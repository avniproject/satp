set role stcatpuat;
insert into encounter( observations,
encounter_date_time,
encounter_type_id,
individual_id,
uuid,
version,
organisation_id,
is_voided,
earliest_visit_date_time,
max_visit_date_time,
name,
created_by_id,
last_modified_by_id,
created_date_time,
last_modified_date_time,
address_id)
select '{}'::json,
null,
(select id from encounter_type where name = 'Common Service Centre - Monthly reporting'),
id,
uuid_generate_v4(),
0,
organisation_id,
false,
'2022-10-01'::timestamp,
'2022-10-11'::timestamp,
'Common Service Centre - Monthly reporting',
(select id from users where username = 'nupoork@stcatpuat'),
(select id from users where username = 'nupoork@stcatpuat'),
now(),
now(),
address_id from individual where subject_type_id = (select id from subject_type where name = 'Common Service Centre');
